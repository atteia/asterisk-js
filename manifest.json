{
  "name": "Mercian Asterisk",
  "description": "An app to connect to the Asterisk API",
  "version": "1",
  "offline_enabled": false,
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["AsteriskAMI.js", "jquery.js", "background.js"]
    }
  },
  "permissions": [
    	"unlimitedStorage",
  	{"socket": ["tcp-connect"]},
  	"notifications",
  	"http://boiler.bluelightstudios.com/"
  ]
}
